    <Script type="xscript5.1"><![CDATA[/**
*  DevPACK
*  @FileName 	statusVM1.xfdl
*  @Creator 	
*  @CreateDate 	
*  @Desction   
************** 소스 수정 이력 ***********************************************
* Date					Modifier					Description
*******************************************************************************

*******************************************************************************
*/

/*******************************************************************************************************************************
 * FORM 변수 선언 영역
*******************************************************************************************************************************/
this.fvCurMonth = "";
/*******************************************************************************************************************************
 * FORM EVENT 영역(onload, onbeforeclose..)
*******************************************************************************************************************************/
this.form_onload = function(obj:nexacro.Form,e:nexacro.LoadEventInfo)
{
	this.gfnFormOnload(obj);
	
	var date = this.gfnGetDate();
	this.fvCurMonth = date.substr(0,6);
	this.divSearch.form.calMonth.set_value(this.fvCurMonth + "31");
	
	this.grdList.set_formatid("EMP");
	
	this.staTitle01.set_text(date.substr(0,4) + "년도 세금계산서 기준 매출현황");
};

//조회
this.cfnSearch = function ()
{
	var selDate = this.divSearch.form.calMonth.value;
	var selYear = selDate.substr(0,4);
	var curYear = this.fvCurMonth.substr(0,4);
	if(curYear > selYear) {
		this.dsParam1.setColumn(0, "END_MONTH", selYear + "1231");
	} else {
		this.dsParam1.setColumn(0, "END_MONTH", this.fvCurMonth + "31");
	}
	this.fnTranSearch();
};


//max size 변경
this.form_onsize = function(obj:nexacro.Form,e:nexacro.SizeEventInfo)
{

};
/*******************************************************************************************************************************
 * 공통함수영역 (cfnSearch : 조회 / cfnSave : 저장 / cfnAdd : 신규 / cfnDel : 삭제 / cfnPrint : 인쇄..)
*******************************************************************************************************************************/

/*******************************************************************************************************************************
 * Transaction 서비스호출 처리 영역
*******************************************************************************************************************************/
this.fnTranSearch = function()
{
	this.gfnInitNextGlobalDataset(this.dsGlobal);
	
	this.dsParam1.applyChange();
	
	// https://nextcns.tobesoft.com/FrontControllerServlet.do?service=xupservice&domain=NEXTp&model=MI_SalesMonthly_S01&format=xml&version=xplatform&compress=false HTTP/1.1
	var strSvcId 	= "month";
	var strSvcUrl 	= "nextService.do";
	var inData 		= "dsGlobal=dsGlobal:A dsParam=dsParam1:A";
	var outData 	= "dsOutput=dsOutput";
	var strArg 		= "trxUrl=" + nexacro.wrapQuote("FrontControllerServlet.do?service=xupservice&domain=NEXTp&model=MI_SalesMonthly_S01&format=xml&version=xplatform&compress=false");
	this.gfnTransaction(strSvcId, strSvcUrl, inData, outData, strArg, "fnCallback");

};

/*******************************************************************************************************************************
 * Callback 영역 (Transaction, popup, message..)
*******************************************************************************************************************************/
//transaction
this.fnCallback = function (sSvcId, nErrCd, sErrMsg, oJson)
{
	switch(sSvcId) {
		case "month":
			break;					
		default:
			break;
	}
};

/*******************************************************************************************************************************
 * 사용자 Function 영역
*******************************************************************************************************************************/

/*******************************************************************************************************************************
 * 각 COMPONENT 별 EVENT 영역
*******************************************************************************************************************************/

this.divCmmnBtn_btnExcelDown_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	// 엑셀 상단 타이틀 텍스트 설정
	var sSubTitle = this.staTitle01.text;
	
	// Excel Export 공통함수 호출
	this.gfnExcelExport(this.grdList, sSubTitle, sSubTitle, "EXCEL2007","",sSubTitle, true, "");	
};

this.divSearch_btnSearch_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	this.cfnSearch();
};

// 구분 변경
this.divSearch_cboType_onitemchanged = function(obj:nexacro.Combo,e:nexacro.ItemChangeEventInfo)
{
	this.grdList.set_formatid(e.postvalue);
	this.dsOutput.clearData();
};

this.divSearch_calMonth_onspin = function(obj:nexacro.Calendar,e:nexacro.CalendarSpinEventInfo)
{
	this.dsOutput.clearData();		
	this.staTitle01.set_text(e.postvalue.substr(0,4) + "년도 세금계산서 기준 매출현황");
};

this.grdList_oncelldblclick = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo)
{
	var objDsId = obj.binddataset;
	var objDs = this.lookup(objDsId);

	var sCellText = obj.getCellProperty("body", e.cell, "text");
	var sMonth = sCellText.split("_")[1];

	if(nexacro.isNumeric(sMonth)) //년합계 제외
	{
		if(sCellText.indexOf("bind:CL_") != -1 && objDs.getColumn(e.row, sCellText.split(":")[1]) != 0)
		{
			var selDate = this.divSearch.form.calMonth.value;
			var selYear = selDate.substr(0,4);
			
			var sTitle = "매출상세";
			var oArg = {
								"END_MONTH" : selYear+sMonth,
								"CONTRACT_TYPE" : objDs.getColumn(objDs.rowposition, "CONTRACT_TYPE"),
								"FORMAT" : this.divSearch.form.cboType.value,
								"DEPT_CD" : objDs.getColumn(objDs.rowposition, "DEPT_CD"),
								"EMP_NO" : objDs.getColumn(objDs.rowposition, "SALES_EMP_NO"),
								"CLIENT_CODE" : objDs.getColumn(objDs.rowposition, "CLIENT_CODE"),
								"PROJECT_CODE" : objDs.getColumn(objDs.rowposition, "PROJECT_CODE"),
								"CONTRACT_NO" : objDs.getColumn(objDs.rowposition, "CONTRACT_NO")
							};
			var oOption = {title:sTitle
						,titlebar:true};
			var sPopupCallBack = "fnPopupCallback";
			this.gfnOpenPopup("orgUserDetail", "sales::next/salesDetailPu.xfdl", oArg, sPopupCallBack, oOption);				
		}
	}	
};
]]></Script>
