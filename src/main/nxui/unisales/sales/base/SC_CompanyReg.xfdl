	this.dsCompany.setColumn(0, "TAG", this.tab_Info.tpg_BasicInfo.form.divTag.getTag());
	
	//저장용 Input Dataset 세팅
 	this.gfnAddInputDataset("dsCompany"			, "dsInput1", "A");
	this.gfnAddInputDataset("dsAddressBook"		, "dsInput2");
