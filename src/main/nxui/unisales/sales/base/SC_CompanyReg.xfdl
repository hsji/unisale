this.fnSave = function()
{		
// 	var sLanguage = nexacro.getApplication().gdsGlobal.getColumn(0, "LANGUAGE");
// 	
// 	if(this.gfnIsEmpty(this.dsCompany.getColumn(0,"COMPANY_NM_JAP")))
